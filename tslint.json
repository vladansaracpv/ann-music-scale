{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "member-access": false,
    "no-console": false,
    "ordered-imports": false,
    "curly": false,
    "no-bitwise": false,
    "interface-name": false,
    "no-namespace": false,
    "ban-types": false,
    "object-literal-sort-keys": false
  }
}
